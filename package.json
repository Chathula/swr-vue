    "dev": "nodemon index.js",
    "dev:watch": "onchange -i -k '**/src/*.ts' -e 'dist/**/*.js' -- yalc push"
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@vue/composition-api": "^0.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.3",
    "nodemon": "^2.0.2",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "tsdx": "^0.12.3",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "yalc": "^1.0.0-pre.35"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
